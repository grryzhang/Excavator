<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>  
    <settings>  
        <!-- 这个配置使全局的映射器启用或禁用缓存 -->  
        <setting name="cacheEnabled" value="true" />  
        <!-- 允许 JDBC 支持生成的键。需要适合的驱动。如果设置为 true 则这个设置强制生成的键被使用，尽管一些驱动拒绝兼容但仍然有效（比如Derby） -->  
        <setting name="useGeneratedKeys" value="true" />  
        <!-- 配置默认的执行器。SIMPLE 执行器没有什么特别之处。REUSE 执行器重用预处理语句。BATCH 执行器重用语句和批量更新  -->  
        <setting name="defaultExecutorType" value="REUSE" />  
        <!-- 全局启用或禁用延迟加载。当禁用时，所有关联对象都会即时加载。 -->  
        <setting name="lazyLoadingEnabled" value="false"/>  
        <!-- 设置超时时间，它决定驱动等待一个数据库响应的时间。  -->  
        <setting name="defaultStatementTimeout" value="25000"/>   
    </settings> 
      
    <!-- 别名配置。
    	  在此配置的别名在其他Mapper配置文件中都可以直接使用alias的内容代替后面type那段长长的全路径说明 
    	 这里的别名配置主要是各个javaBean的别名
    -->  
    <typeAliases>  
    	<typeAlias alias="NC_Corporation"                type="com.zhongzhou.Excavator.model.NC.CorporationDoc" />
    	<typeAlias alias="Corporation"                   type="com.zhongzhou.Excavator.model.Corporation" />
    	<typeAlias alias="CorporationSearchParameters"   type="com.zhongzhou.Excavator.model.CorporationSearchParameters" />
    	<typeAlias alias="CorporationIntegrationMapping" type ="com.zhongzhou.Excavator.model.CorporationIntegrationMapping" />
    	<typeAlias alias="CorporationIntegrationMappingSearchParameters" 
    			   type ="com.zhongzhou.Excavator.model.CorporationIntegrationMappingSearchParameters" />
    			   
    			       	
    	<typeAlias alias="NC_ItemCategory"                       type="com.zhongzhou.Excavator.model.NC.ItemCategory" />
    	<typeAlias alias="NC_ItemCategorySearchParameters"       type="com.zhongzhou.Excavator.model.NC.ItemCategorySearchParameters" />
    	<typeAlias alias="ItemCategory"                		     type="com.zhongzhou.Excavator.model.ItemCategory" />
    	<typeAlias alias="ItemCategorySearchParameters"          type="com.zhongzhou.Excavator.model.ItemCategorySearchParameters" />
    	<typeAlias alias="ItemCategoryMapping"                   type="com.zhongzhou.Excavator.model.ItemCategoryMapping" />
    	<typeAlias alias="ItemCategoryMappingSearchParameters"   type="com.zhongzhou.Excavator.model.ItemCategoryMappingSearchParameters" />
    	
    	<typeAlias alias="NC_Item"                      		type="com.zhongzhou.Excavator.model.NC.Item" />
    	<typeAlias alias="NC_ItemSearchParameters"      		type="com.zhongzhou.Excavator.model.NC.ItemSearchParameters" />
    	<typeAlias alias="Item"                         		type="com.zhongzhou.Excavator.model.Item" />
    	<typeAlias alias="ItemSearchParameters"         		type="com.zhongzhou.Excavator.model.ItemSearchParameters" />
    	<typeAlias alias="ItemMapping"                  		type="com.zhongzhou.Excavator.model.ItemMapping" />
    	<typeAlias alias="ItemMappingSearchParameters"          type="com.zhongzhou.Excavator.model.ItemMappingSearchParameters" />
    	<typeAlias alias="ItemUnit"             		        type="com.zhongzhou.Excavator.model.ItemUnit"                          />
    	<typeAlias alias="ItemUnitMapping"                      type="com.zhongzhou.Excavator.model.ItemUnitMapping"                   />
    	<typeAlias alias="ItemUnitMappingSearchParameters"      type="com.zhongzhou.Excavator.model.ItemUnitMappingSearchParameters"   />
    	
    	<typeAlias alias="NC_Price"          		     		type="com.zhongzhou.Excavator.model.NC.Price"                       />
    	<typeAlias alias="NC_PriceSearchParameters"      		type="com.zhongzhou.Excavator.model.NC.PriceSearchParameters"       />
    	<typeAlias alias="NC_PriceCategory"              		type="com.zhongzhou.Excavator.model.NC.PriceCategory" 			     />
    	<typeAlias alias="Price"             		     		type="com.zhongzhou.Excavator.model.Price" 					     />
    	<typeAlias alias="PriceMapping"                  		type="com.zhongzhou.Excavator.model.PriceMapping" 				     />
    	<typeAlias alias="PriceMappingSearchParameters"  		type="com.zhongzhou.Excavator.model.PriceMappingSearchParameters" 	 />
    	<typeAlias alias="PriceCategory"                        type="com.zhongzhou.Excavator.model.PriceCategory" 			     />
    	<typeAlias alias="PriceCategoryMapping"                 type="com.zhongzhou.Excavator.model.PriceCategoryMapping"           />
    	<typeAlias alias="PriceCategoryMappingSearchParameters" type="com.zhongzhou.Excavator.model.PriceCategoryMapping"           />
    	
    	<typeAlias alias="Currency"             		        type="com.zhongzhou.Excavator.model.Currency"                          />
    	<typeAlias alias="CurrencyMapping"                      type="com.zhongzhou.Excavator.model.CurrencyMapping"                   />
    	<typeAlias alias="CurrencyMappingSearchParameters"      type="com.zhongzhou.Excavator.model.CurrencyMappingSearchParameters"   />
    	
    </typeAliases> 
 
      
    <!-- 指定映射器路径 -->  
    <!-- mapper 目录下记录了所有的sql语句的配置, 对应的类似SysUser.xml就是存储了对应于sys_user这张表的sql语句 -->
    <mappers>  
    	<mapper resource="mapper/oracle/NC/Corporation.xml" />
    	<mapper resource="mapper/oracle/NC/Item.xml" />
    	<mapper resource="mapper/oracle/NC/Price.xml" />
    	
    	<mapper resource="mapper/postgresql/masterdata/Corporation.xml" />    
    	<mapper resource="mapper/postgresql/masterdata/Item.xml" />
    	<mapper resource="mapper/postgresql/masterdata/Price.xml" />
    	<mapper resource="mapper/postgresql/masterdata/Currency.xml" />
    </mappers>  
</configuration>   